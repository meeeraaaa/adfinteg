{
	"name": "quizJoining",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "quizSQL",
						"type": "DatasetReference"
					},
					"name": "quiz"
				},
				{
					"dataset": {
						"referenceName": "quizQuestionSQL",
						"type": "DatasetReference"
					},
					"name": "quizQUestion"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          quizID as string,",
				"          InstructorID as string,",
				"          quizName as string,",
				"          dateOpen as string,",
				"          dateClose as string,",
				"          quizDescription as string,",
				"          quizCode as string,",
				"          classAverage as string,",
				"          numStudents as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> quiz",
				"source(output(",
				"          quizQuestionID as string,",
				"          questionName as string,",
				"          answer1 as string,",
				"          answer2 as string,",
				"          answer3 as string,",
				"          quizID as string,",
				"          correctAnswer as string,",
				"          questionType as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> quizQUestion",
				"quiz, quizQUestion join(quiz@quizID == quizQUestion@quizID,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}