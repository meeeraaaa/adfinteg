{
	"name": "quiz_FS",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "quizQuestionSQL",
						"type": "DatasetReference"
					},
					"name": "quizQuestions"
				},
				{
					"dataset": {
						"referenceName": "quizSQL",
						"type": "DatasetReference"
					},
					"name": "quiz"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          quizQuestionID as string,",
				"          questionName as string,",
				"          answer1 as string,",
				"          answer2 as string,",
				"          answer3 as string,",
				"          quizID as string,",
				"          correctAnswer as string,",
				"          questionType as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> quizQuestions",
				"source(output(",
				"          quizID as string,",
				"          InstructorID as string,",
				"          quizName as string,",
				"          dateOpen as string,",
				"          dateClose as string,",
				"          quizDescription as string,",
				"          quizCode as string,",
				"          classAverage as string,",
				"          numStudents as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> quiz"
			]
		}
	}
}