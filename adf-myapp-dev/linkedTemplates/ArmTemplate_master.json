{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-myapp-dev"
		},
		"BlobStorage_LS_test_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage_LS_test'"
		},
		"LS_ETL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ETL'"
		},
		"LS_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL'"
		},
		"LS_blob_SQL_new_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blob_SQL_new'"
		},
		"LS_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-serverdemo.database.windows.net"
		},
		"LS_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "purchasings"
		},
		"LS_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ananyasarkar"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-myapp-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BlobStorage_LS_test_connectionString": {
						"value": "[parameters('BlobStorage_LS_test_connectionString')]"
					},
					"LS_ETL_connectionString": {
						"value": "[parameters('LS_ETL_connectionString')]"
					},
					"LS_SQL_password": {
						"value": "[parameters('LS_SQL_password')]"
					},
					"LS_blob_SQL_new_connectionString": {
						"value": "[parameters('LS_blob_SQL_new_connectionString')]"
					},
					"LS_SQL_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_properties_typeProperties_server')]"
					},
					"LS_SQL_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_properties_typeProperties_database')]"
					},
					"LS_SQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}